//How many such routes are there through a 20Ã—20 grid?


#include <iostream>
using namespace std;

int main ()
{
    unsigned long long int sum = 1;
    int n = 2 * 20;
    for ( int i = 1 ; i <= n / 2 ; i++ )
    {
        sum*= ( i + n / 2 );
        sum /= i;
    }
    cout << sum;
    return 0;
}
