//Find the difference between the sum of the squares of the first one hundred natural numbers and the square of the sum.


#include <iostream>
using namespace std;

int main ()
{
    int x;
    int sum , sumSQ;
    for ( x = 1 , sum = 0 , sumSQ = 0 ; x <= 100 ; x++ )
    {
        sumSQ += x * x;
        sum += x;
    }
    cout << sum * sum - sumSQ;
    return 0;
}
