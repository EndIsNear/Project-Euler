// What is the smallest positive number that is evenly divisible by all of the numbers from 1 to 20?


#include <iostream>
using namespace std;

main ()
{
    unsigned int a;
    for ( a = 20 ; 1 ; a += 20 )
        if ( a % 20 == 0 && a % 19 == 0 && a % 18 == 0 && a % 17 == 0 && a % 16 == 0
            && a % 15 == 0 && a % 14 == 0 && a % 13 == 0 && a % 12 == 0 && a % 11 == 0 )
            {
                cout << a;
                break;
            }
    return 0;
}
